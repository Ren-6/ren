commands: [
	["quit",     "pkill ren",   "Q"],
	["browser",  "qutebrowser", "B"],
	["terminal", "st",          "T"],
	["htop",     "st -e htop",  "H"],
	["editor",   "subl",        "E"],
]

font-size: 24
font-path: "JuliaMono.ttf"

exit-on-key: 1
window-width: 480
spacing: 6

background-color: "#242430"
command-color:    "#696969"
text-color:       "#fafafa"
key-color:        "#fba0c0"

